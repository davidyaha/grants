{
    "parser": "babel-eslint",
    "plugins": [
        "flowtype"
    ],
    "env": {
        "jest": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "error",
        "no-unreachable": "error",
        "no-unused-vars": "error",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "semi": "error"
    }
}